{
    "name": "bacart/transliteration",
    "description": "Bacart transliteration library",
    "type": "library",
    "keywords": [
        "php",
        "transliteration",
        "bacart"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpstan/phpstan": "^0.11",
        "rector/rector": "^0.4"
    },
    "suggest": {
        "friendsofphp/php-cs-fixer": "A tool to automatically fix PHP Coding Standards issues",
        "phpstan/phpstan": "PHPStan - PHP Static Analysis Tool",
        "rector/rector": "Instant upgrade and refactoring of your PHP code"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Bacart\\Transliteration\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Alex Bacart",
            "email": "alex@bacart.net"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    }
}
